FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     particleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         3.11;

deltaT          2e-3;

writeControl    runTime;

writeInterval   0.01;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustableTimeStep yes;
maxCo 0.3;

// ************************************************************************* //
